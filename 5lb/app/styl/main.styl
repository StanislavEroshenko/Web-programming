.UpThings
 display:grid
 font-size: 1.0vw 
 grid-column-gap : 3%
 grid-template-columns: repeat(8, 1fr)
 grid-template-rows: repeat(4, 1fr)
.UpThings > .UpColorFirst
 background-color: gray
 grid-column: 1 / 9
 grid-row: 1/2
.UpThings > .UpColorSecond
 background-color: lightblue
 grid-column: 1 / 9
 grid-row: 2/5
.UpThings > .Search
 grid-column: 5 / 9
 grid-row: 1/2
 text-align:right
.UpThings > .TopName
 grid-column: 4 / 7
 grid-row: 2/3
 text-align: left
.UpThings  > .Menu1
 grid-column: 5 / 6
 grid-row: 4/5
 text-align: center
 background-color: gray
 height: 110%
.UpThings  > .Menu1 a
 text-decoration: none
 color: black
.UpThings  > .Menu1:hover
 display: block
 width: 100%
 height: 110%
 background-color: lightgray
.UpThings  > .Menu2
 grid-column: 6 / 7
 grid-row: 4/5
 text-align: center
.UpThings  > .Menu2 a
 text-decoration: none
 color: black
.UpThings  > .Menu2:hover
 display: block
 width: 100%
 height: 110%
 background-color: lightgray
.UpThings  > .Menu3
 grid-column: 7 / 8
 grid-row: 4/5
 text-align: center
.UpThings  > .Menu3 a
 text-decoration: none
 color: black
.UpThings  > .Menu3:hover
 display: block
 width: 100%
 height: 110%
 background-color: lightgray

.Last
 display:grid
 grid-template-columns: 2fr 2fr
 grid-template-rows: 1fr
 font-size: 1.0vw
.Last > .LastFIO
 text-align:left
 grid-column: 1/2
 grid-row: 1/2
.Last > .LastContacts
 text-align:right
 grid-column: 2/3
 grid-row: 1/2

.menu1
 display: grid
 width: 100%
 grid-template-columns: repeat(8, 1fr)
 grid-template-rows: 20vw 2fr
 font-size: 1.0vw
 grid-gap: 1vw
 padding: 3%
.menu1  > .ItemPicture1
 grid-column: 1 / 6
 grid-row: 1/2
 overflow: hidden
 padding: 1%
.menu1 > .ItemPicture2
 grid-column: 6 / 9
 grid-row: 1/2
 overflow: hidden
 padding: 1%
.menu1 > .ItemText1
 overflow: hidden
 text-overflow: ellipsis
 grid-column: 1 / 6
 grid-row: 2/3
 padding: 1%
.menu1 > .ItemText2
 overflow: hidden
 text-overflow: ellipsis
 grid-column: 6 / 9
 grid-row: 2/3
 padding: 1%
.Border1
 border: solid black
 grid-column: 1 / 6
 grid-row: 1/3
.Border2
 border:solid black
 grid-column: 6 / 9
 grid-row: 1/3

.menu2
 display: grid
 width: 100%
 grid-template-columns: repeat(4, 1fr)
 grid-template-rows: repeat(2, 11vw)
 font-size: 1.0vw
 grid-gap: 1vw
 padding: 3%
.menu2  > .ItemPicture3
 grid-column: 1 / 2
 grid-row: 1/2
 overflow: hidden
.menu2  > .ItemPicture4
 grid-column: 3/4
 grid-row: 1/2
 overflow: hidden
.menu2  > .ItemPicture5
 grid-column: 1 / 2
 grid-row: 2/3
 overflow: hidden
.menu2  > .ItemPicture6
 grid-column: 3 / 4
 grid-row: 2/3
 overflow: hidden
.menu2 > .ItemText3
 overflow: hidden
 text-overflow: ellipsis
 grid-column: 2 / 3
 grid-row: 1/2
.menu2 > .ItemText4
 overflow: hidden
 text-overflow: ellipsis
 grid-column: 4 / 5
 grid-row: 1/2
.menu2 > .ItemText5
 overflow: hidden
 text-overflow: ellipsis
 grid-column: 2 / 3
 grid-row: 2/3
.menu2 > .ItemText6
 overflow: hidden
 text-overflow: ellipsis
 grid-column: 4/5
 grid-row: 2/3


@media all and (max-width: 600px)
 .Last
  display:grid
  grid-template-columns: 2fr 2fr
  grid-template-rows: 1fr
  font-size: 2.0vw
 
 .menu1
  display: grid
  width: 100%
  grid-template-columns: 1fr
  grid-template-rows: repeat(4, 20vw)
  font-size: 2.0vw
  grid-gap: 1vw
  padding: 3%
 .menu1  > .ItemPicture1
  grid-column: 1 / 2
  grid-row: 1/2
  overflow: hidden
  padding: 1%
 .menu1 > .ItemPicture2
  grid-column: 1 / 2
  grid-row: 3/4
  overflow: hidden
  padding: 1%
 .menu1 > .ItemText1
  overflow: hidden
  text-overflow: ellipsis
  grid-column: 1/2
  grid-row: 2/3
  padding: 1%
 .menu1 > .ItemText2
  overflow: hidden
  text-overflow: ellipsis
  grid-column: 1/2
  grid-row: 4/5
  padding: 1%
 .Border1
  border: solid black
  grid-column: 1 / 2
  grid-row: 1/3
 .Border2
  border:solid black
  grid-column: 1/2
  grid-row: 3/5

 .menu2
  display: grid
  width: 100%
  grid-template-columns: 1fr
  grid-template-rows: repeat(8, 20vw)
  font-size: 2.0vw
  grid-gap: 1vw
  padding: 3%
 .menu2  > .ItemPicture3
  grid-column: 1 / 2
  grid-row: 1/2
  overflow: hidden
 .menu2  > .ItemPicture4
  grid-column: 1 / 2
  grid-row: 3/4
  overflow: hidden
 .menu2  > .ItemPicture5
  grid-column: 1 / 2
  grid-row: 5/6
  overflow: hidden
 .menu2  > .ItemPicture6
  grid-column: 1 / 2
  grid-row: 7/8
  overflow: hidden
 .menu2 > .ItemText3
  overflow: hidden
  text-overflow: ellipsis
  grid-column: 1 / 2
  grid-row: 2/3
 .menu2 > .ItemText4
  overflow: hidden
  text-overflow: ellipsis
  grid-column: 1 / 2
  grid-row: 4/5
 .menu2 > .ItemText5
  overflow: hidden
  text-overflow: ellipsis
  grid-column: 1 / 2
  grid-row: 6/7
 .menu2 > .ItemText6
  overflow: hidden
  text-overflow: ellipsis
  grid-column: 1 / 2
  grid-row: 8/9

 .UpThings
  width: 100%
  display:grid
  font-size: 3.5vw
  grid-column-gap : 3%
  grid-template-columns: repeat(3, 1fr)
  grid-template-rows: repeat(3, 1fr)
 .UpThings > .Search
  display: none
 .UpThings > .TopName
  display: none
  grid-column: 1 / 4
  grid-row: 2/3
  text-align: center
 .UpThings  > .Menu1
  width: 120%
  grid-column: 1 / 4
  grid-row: 1/2
  text-align: center
 .UpThings  > .Menu2
  width: 120%
  grid-column: 1 / 4
  grid-row: 2/3
  text-align: center
 .UpThings  > .Menu3
  width: 120%
  grid-column: 1 / 4
  grid-row: 3/4
  text-align: center
 .UpThings > .UpColorFirst
  display: block
  background-color: white
  text-align: center
  grid-column: 2 / 3
  grid-row: 1/2
 .UpThings > .UpColorSecond
  display: block
  background-color: white
 .UpThings  > .Menu3:hover
  display: block
  width: 120%
  background-color: white
 .UpThings  > .Menu1:hover
  display: block
  width: 120%
  background-color: white
 .UpThings  > .Menu2:hover
  display: block
  width: 120%
  background-color: white
 .UpThings > .Search
  grid-column: 2 / 3
  grid-row: 1/2
  text-align:center
 .UpThings  > .Menu1 a
  text-decoration: none
  color: black
  background-color: gray

